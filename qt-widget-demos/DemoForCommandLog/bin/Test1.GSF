        <o:ColSetting DisplayWidth="80"/>
        <o:ColSetting DisplayWidth="80"/>
        <o:ColSetting DisplayWidth="80"/>
        <o:ColSetting DisplayWidth="80"/>
        <o:ColSetting DisplayWidth="80"/>
