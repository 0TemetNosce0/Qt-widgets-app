                                       stop: 0 #E0E0E0, stop: 1 #FFFFFF);
}

QDockWidget {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                       stop: 0 #E0E0E0, stop: 1 #FFFFFF);
    background-image: url(:/img/pagefold.png);
    background-position: top right;
    background-repeat: no-repeat
}

/*/////////////////////////////////////////////////////////////////////////*/
QLineEdit
{
	background: qlineargradient(
				x1: 0, y1: 0, x2: 0, y2: 1,
				stop: 0 #f6f6f6, stop: 0.5 #ececec,
                stop: 0.51 #e1e1e1, stop: 1.0 #fcfcfc);
				border: 1px solid #8b8b8b;
}

QLineEdit[echoMode="2"] {
    lineedit-password-character: 9679;
}

/*/////////////////////////////////////////////////////////////////////////*/
QTextEdit
{
	border: 1px solid #8b8b8b;
	border-style: solid;
	border-radius: 5px;
	min-height: 22px;
}

/*/////////////////////////////////////////////////////////////////////////*/
QAbstractSpinBox 
{
	background: qlineargradient(
				x1: 0, y1: 0, x2: 0, y2: 1,
				stop: 0 #f6f6f6, stop: 0.5 #ececec,
                stop: 0.51 #e1e1e1, stop: 1.0 #fcfcfc);
	border: 1px solid #8b8b8b;
}

QAbstractSpinBox::up-button, QAbstractSpinBox::down-button
{
	border-width: 1px;
	border-style: solid;
	border-color: transparent;
	border-left-color: #D3D3D3;
	
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0 #eeeeee, stop: 0.4 #e6e6e6,
                                 stop: 0.5 #ececec, stop: 1.0 #eeeeee);
                
	width: 22px;
}

QAbstractSpinBox::up-button:hover, QAbstractSpinBox::down-button:hover
{
	background: qlineargradient(
				x1: 0, y1: 0, x2: 0, y2: 1,
				stop: 0 #bed4ef, stop: 0.5 #8ebdec,
                stop: 0.51 #6bb0f6, stop: 1.0 #97daff);
                
	border-color: #007f7f;
	border-style: solid;
}

QAbstractSpinBox::up-button:pressed, QAbstractSpinBox::down-button:pressed
{
	padding-top: 1px;
}

QAbstractSpinBox::up-arrow
{
	image: url(:/img/combo_up_arrow.png);
}

QAbstractSpinBox::down-arrow
{
	image: url(:/img/combo_down_arrow.png);
}

